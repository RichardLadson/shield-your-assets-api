# Backend Environment Configuration Example
# Copy this file to .env and update with your actual values

# Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_NAME=medicaid_planning
DB_USER=your_database_user
DB_PASSWORD=your_secure_database_password

# Application Configuration
JWT_SECRET=your_super_long_random_jwt_secret_here_64_characters_minimum
NODE_ENV=development
PORT=3001

# CORS Configuration
CORS_ORIGIN=http://localhost:8080,https://yourdomain.com

# Optional: Enable query logging for development
DB_LOGGING=true

# AWS Configuration (for production)
AWS_REGION=us-east-1
RDS_ENDPOINT=your_rds_endpoint
APP_URL=http://localhost:3001

# Optional: Default planner ID fallback
DEFAULT_PLANNER_ID=1

# CRM Integration (optional)
GHL_API_KEY=your_gohighlevel_api_key

# GoHighLevel OAuth Configuration (optional)
GHL_CLIENT_ID=your_gohighlevel_client_id
GHL_CLIENT_SECRET=your_gohighlevel_client_secret
GHL_REDIRECT_URI=http://localhost:3001/api/ghl/callback

# Logging
LOG_LEVEL=info

# Data Paths
DATA_PATH=./src/data